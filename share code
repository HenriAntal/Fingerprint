   public static boolean identical(boolean[][] image1, boolean[][] image2) {
	  //TODO implement
	  boolean stop = true;
	  for (int i = 0; i < image1.length; ++i) {
		  for (int j = 0; j < image1[0].length; ++j) {
			  if (image1[i][j] != image2[i][j]) {
				  stop = false;
			  }
		  }
	  }
		  
	  return stop;
  }
